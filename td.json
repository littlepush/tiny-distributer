{
	"tcprelay-server1": {
		"server": "TCPRELAY",
		"port": 38423,
		"socks5": {
			"addr": "127.0.0.1",
			"port": 4098
		}
	},
	"tcprelay-server2": {
		"server": "TCPRELAY",
		"bind": "192.168.1.251",
		"port": 38420,
		"src": [
			"192.168.1.0/24",
			"10.10.64.0/24",
			"127.0.0.1"
		]
	},
	"redirect-server2": {
		"server": "REDIRECT",
		"port": 38424,
		"src": ["127.0.0.1"],
		"dst": [
			{
				"addr": "192.168.1.250",
				"port": 3389,
				"socks5": [
					{
						"addr": "127.0.0.1", 
						"port": 1080
					}
				]
			},
			{
				"addr": "192.168.1.248",
				"port": 3389
			},
			{
				"addr": "192.168.1.249",
				"port": 3389,
				"reply": false
			}
		]
	},
	"socks5-server": {
		"server": "SOCKS5",
		"port": 4001,
		"socks5": {
			"addr": "127.0.0.1",
			"port": 1080
		}
	},
	"backdoor-server": {
		"server": "BACKDOOR",
		"port": 4002,
		"listen": "socks5-server",
		"request": true,
		"response": false
	}
}
